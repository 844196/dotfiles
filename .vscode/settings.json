{
  "files.associations": {
    "**/files/dot_vimrc": "vimrc",
    "**/files/dot_config/zsh/**": "shellscript",
    "**/files/dot_config/git/config": "properties",
    "**/files/dot_config/git/ignore": "ignore",
    "**/files/dot_local/share/zsh/**": "shellscript",
    "*.sh.tmpl": "shellscript",
    "*.yaml.tmpl": "yaml",
  },
  "cSpell.words": [
    "chezmoi",
  ],
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "denoland.vscode-deno",
  },
  "code-runner.executorMap": {
    "typescript": " deno run -A",
  },
  "code-runner.runInTerminal": true
}
