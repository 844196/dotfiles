readonly local GHQ="$(aqua which ghq)"
readonly local ONEFETCH="$(aqua which onefetch)"

readonly local __PREVIEW__="
$GHQ list --full-path --exact {} | xargs -I REPO $ONEFETCH --show-logo never REPO
"

ghq list \
  | dot-fzf \
    --no-exit-0 \
    --no-multi \
    --scheme=path \
    --preview $__PREVIEW__ \
  | xargs -I PATH ghq list --full-path --exact PATH \
  | dot-cd
