# vim: set ft=zsh:

tput cub 9999

anyframe-source-git-branch \
  | SHELL==$ZSH_NAME anyframe-selector-fzf '' \
    --multi \
    --delimiter ' ' \
    --preview 'git --no-pager graph -n $(( FZF_PREVIEW_LINES / 2 )) {1} | emojify' \
  | cut -d ' ' -f 1,1 \
  | anyframe-action-insert

zle .reset-prompt
