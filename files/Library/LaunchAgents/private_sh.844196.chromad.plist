<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>sh.844196.chromad</string>

    <key>ProgramArguments</key>
    <array>
      <string>/bin/sh</string>
      <string>-c</string>
      <string>CHROMA_RUNTIME_DIR=`getconf DARWIN_USER_TEMP_DIR`chroma; export CHROMA_RUNTIME_DIR; mkdir -p $CHROMA_RUNTIME_DIR; ~/.local/share/mise/shims/chromad</string>
    </array>

    <key>StandardOutPath</key>
    <string>/tmp/chromad.stdout</string>
    <key>StandardErrorPath</key>
    <string>/tmp/chromad.stderr</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>
  </dict>
</plist>
