" set encoding UTF-8
set encoding=utf-8
scriptencoding utf-8

" 行数指定
set lines=52

" フォント
set guifont=Ricty\ Nerd\ 10

" gvimの時はフォントを綺麗にする
set antialias

" スクロールバーとか表示しない
set guioptions=NONE

" クリップボード連携
set clipboard&
set clipboard+=unnamed

" ち～ん（笑）って鳴らさない
set visualbell t_vb=

set bg=dark
colorscheme hybrid
set linespace=4
