#!/bin/bash

a=$(printf "\x1b[48;05;232m   \x1b[0m") # 黒
b=$(printf "\x1b[48;05;79m   \x1b[0m") # 緑
c=$(printf "\x1b[48;05;62m   \x1b[0m") # 紫
d=$(printf "\x1b[48;05;254m   \x1b[0m") # 白1
e=$(printf "\x1b[48;05;246m   \x1b[0m") # 白2
f=$(printf "\x1b[48;05;249m   \x1b[0m") # 白3
g=$(printf "\x1b[48;05;237m   \x1b[0m") # グレー1
h=$(printf "\x1b[48;05;239m   \x1b[0m") # グレー2
i=$(printf "\x1b[48;05;241m   \x1b[0m") # グレー3
j=$(printf "\x1b[48;05;223m   \x1b[0m") # 肌
k=$(printf "\x1b[48;05;124m   \x1b[0m") # 赤1
l=$(printf "\x1b[48;05;160m   \x1b[0m") # 赤2
m=$(printf "\x1b[48;05;95m   \x1b[0m") # 茶

cat <<EOF
      $a   $a$a$a$a$a$a$a
   $a$b$a$e$f$f$f$f$f$e$a
   $a$c$e$e$e$e$e$e$e$e$e$a
   $a$d$e$f$g$g$f$f$f$f$e$a
   $a$e$a$a$a$a$a$a$a$a$a$e$a
$a$e$a$i$g$j$g$i$g$g$i$i$a$e$a
   $a$h$h$j$a$j$g$j$a$g$h$g$a
   $a$g$h$j$a$j$j$j$a$j$g$g$a
      $a$g$a$j$j$j$j$j$a$g$a
         $a$g$a$a$j$a$a$g$a
      $a$a$k$l$k$g$k$l$k$a$a
   $a$g$j$a$k$l$l$l$k$a$j$g$a
   $a$a$a$e$a$k$k$k$a$e$a$a$a
         $a$f$f$f$e$f$f$f$a
         $a$k$l$k$a$k$l$k$a
            $a$m$a   $a$m$a
EOF
