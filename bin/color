#!/bin/bash

d_blk=$(printf "\x1b[48;05;0m")
d_red=$(printf "\x1b[48;05;1m")
d_grn=$(printf "\x1b[48;05;2m")
d_yel=$(printf "\x1b[48;05;3m")
d_blu=$(printf "\x1b[48;05;4m")
d_mag=$(printf "\x1b[48;05;5m")
d_cyn=$(printf "\x1b[48;05;6m")
d_wht=$(printf "\x1b[48;05;7m")
l_blk=$(printf "\x1b[48;05;8m")
l_red=$(printf "\x1b[48;05;9m")
l_grn=$(printf "\x1b[48;05;10m")
l_yel=$(printf "\x1b[48;05;11m")
l_blu=$(printf "\x1b[48;05;12m")
l_mag=$(printf "\x1b[48;05;13m")
l_cyn=$(printf "\x1b[48;05;14m")
l_wht=$(printf "\x1b[48;05;15m")
e=$(printf "\x1b[0m")

cat <<EOF
$d_red      $e    $d_grn      $e    $d_yel      $e    $d_blu      $e    $d_mag      $e    $d_cyn      $e    $d_wht      $e    $d_blk      $e
$d_red      $e$l_red  $e  $d_grn      $e$l_grn  $e  $d_yel      $e$l_yel  $e  $d_blu      $e$l_blu  $e  $d_mag      $e$l_mag  $e  $d_cyn      $e$l_cyn  $e  $d_wht      $e$l_wht  $e  $d_blk      $e$l_blk  $e
$d_red      $e$l_red  $e  $d_grn      $e$l_grn  $e  $d_yel      $e$l_yel  $e  $d_blu      $e$l_blu  $e  $d_mag      $e$l_mag  $e  $d_cyn      $e$l_cyn  $e  $d_wht      $e$l_wht  $e  $d_blk      $e$l_blk  $e
  $l_red      $e    $l_grn      $e    $l_yel      $e    $l_blu      $e    $l_mag      $e    $l_cyn      $e    $l_wht      $e    $l_blk      $e
EOF
